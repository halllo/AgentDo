{
	"Serilog": {
		"Using": [
			"Serilog.Sinks.Console"
		],
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"System": "Warning",
				"Microsoft": "Warning"
			}
		},
		"WriteTo": [
			{
				"Name": "Console",
				"Args": {
					"theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
					"outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {ThreadId} {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
				}
			}
		],
		"Enrich": [ "FromLogContext", "WithThreadId" ]
	},

	"Prompts": [
		"What can you do?",
		"Summarize the this converation. Make it as consize as possible. The shorter, the better."
	],
	"McpServers": [
	],

	"AWSBedrockAccessKeyId": "",
	"AWSBedrockSecretAccessKey": "",
	"AWSBedrockRegion": "",
	"AWSBedrockModelId": "eu.anthropic.claude-sonnet-4-20250514-v1:0",
}